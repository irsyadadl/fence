{
    "name": "irsyadadl/fence",
    "description": "Fence is a Laravel views for Laravel Fortify",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel",
        "fence"
    ],
    "authors": [
        {
            "name": "Irsyad A. Panjaitan",
            "email": "irsyad@parsinta.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0",
        "laravel/fortify": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "Irsyadadl\\Fence\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Irsyadadl\\Fence\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x"
        },
        "laravel": {
            "providers": [
                "Irsyadadl\\Fence\\FenceServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    }
}
